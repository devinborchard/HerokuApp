{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "start:dev": "cross-env concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start:local": "cross-env concurrently \"npm run server:local\" \"npm run client:local\"",
    "server": "node .",
    "server:dev": "cross-env NODE_ENV=development nodemon .",
    "server:local": "cross-env NODE_ENV=local nodemon index.js --ignore client/",
    "server:debug": "cross-env NODE_ENV=local nodemon index.js --ignore client/ --inspect --port=7722",
    "server:locally": "cross-env NODE_ENV=local node --inspect=0.0.0.0:9054 index.js --ignore client/",
    "server:local:ws": "cross-env NODE_ENV=local nodemon index.js --ignore client/",
    "client:dev": "cd client && npm run start:dev",
    "client:local": "cd client && cross-env REACT_APP_ENV=local npm run start",
    "heroku-postbuild": "sh -ac 'npm run db:migrate && npm run db:seed && cd client && npm install && npm run build'",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db:migrate:local": "cross-env NODE_ENV=local npm run db:migrate",
    "db:migrate:local:undo": "cross-env NODE_ENV=local npm run db:migrate:undo",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:local": "cross-env NODE_ENV=local npm run db:seed",
    "db:seed:local:undo": "cross-env NODE_ENV=local npm run db:seed:undo",
    "migration": "node_modules/.bin/sequelize migration:generate --name",
    "seed": "node_modules/.bin/sequelize seed:generate --name",
    "commit": "bash ./devtools/commands/commit.sh"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "react-router-dom": "^6.3.0"
  }
}
